<div class="container_full" id="catalogSection">
    <div class="container body-content">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <h3 style="text-align:center">En Son Eklenen İlanlarımız</h3>
                <p style="text-align:center">Bu alanda ilanlarımız karışık olarak listelenmektedir.</p>
            </div>
        </div>

        

        <div  class="row" ng-controller="InfinitySearchController" infinite-scroll='NextPage()' >
            <div class="col-md-4" ng-repeat="emp in EmployeeList" >
                <div class="thumbnail" style="border:1px solid #eee; background-color:#f5f5f5">
                   
                    <img class="img-rounded" lazy-src="http://uruninsaat.com/Content/Images/1.jpg" animate-visible="true" animate-speed="0.5s" alt="" />
                    <div class="caption">
                        <h5>
                        {{emp.id}} =  {{emp.baslik}}
                        </h5>
                        <p>
                           {{emp.fiyat}}

                        </p>

                        <p><a href="{{emp.id}}">Devamı..</a></p>
                    </div>
                </div>
            </div>
            

        </div>

    </div>
</div>
